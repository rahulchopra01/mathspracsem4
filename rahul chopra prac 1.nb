             RowBox[{
              RowBox[{"(", 
               RowBox[{"x1", "-", "x0"}], ")"}], "/", "2"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"f", "[", "m", "]"}], "*", 
               RowBox[{"f", "[", "x1", "]"}]}], ">", "0"}], ",", " ", 
             RowBox[{"x1", "=", "m"}], ",", " ", 
